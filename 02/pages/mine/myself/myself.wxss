  left: 40rpx;
}
.myFavorite_me,.myFavorite_set{
  font-size: 28rpx;
  font-weight: bold;
	color: #333333;
}
.myFavorite_set{
  margin-left: 13rpx;
}
.myFavorite_num{
  font-size: 40rpx;
