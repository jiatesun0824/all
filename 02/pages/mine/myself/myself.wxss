.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    width: 100%;
}

.flex-iteam {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

.head-portrait {
	  position: relative;
    width: 100%;
    height: 260rpx;
    background-color: #ffffff;
    overflow: hidden;
    border-bottom: 18rpx solid #f5f5f5;
}


.head-info{
	width: 100%;
	height: 200rpx;
	background-color: #ffffff;
  padding: 85rpx 40rpx;
  padding-left: 185rpx;
  padding-right: 80rpx;
  box-sizing: border-box;
  position: relative;
}
.head-info>image.userImg{
  width: 110rpx;
	height: 110rpx;
  position: absolute;
  left: 40rpx;
  top: 65rpx;
  border-radius: 50%;
  background: #e8e8e8;
}
.head-info>view{
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  min-height: 26rpx;
  line-height: 1;
}
.head-info>view.p1{
  font-family: PingFang-SC-Bold;
	font-size: 32rpx;
	font-weight: 600;
	color: #403834;
  margin-bottom: 20rpx;
  line-height: 36rpx;
}
.head-info>view.p2{
  font-family: PingFang-SC-Bold;
	font-size: 26rpx;
	font-weight: 600;
	color: #ffaa33;
  position: relative;
  padding-left: 40rpx;
}
.head-info>view.p2 image{
  position: absolute;
  left: 0;
  top:1rpx;
  height: 24rpx;
  width: 25rpx;
}
.head-info>image.goNext{
  width: 15rpx;
	height: 28rpx;
  position: absolute;
  top: 107rpx;
  right: 40rpx;
}


.head-iteam{
	margin:0 auto;
	margin-top: 18rpx;
	text-align: center;

}
.head-iteam.image{
	position: relative;
	margin:0 auto;
	margin-top: 32rpx;
	width: 148rpx;
	height: 148rpx;
	border-radius: 100%;
}
.head-iteam.name{
	font-size: 32rpx;
	line-height: 1;
	margin-top: 20rpx;
	color: #333333;
}
.head-iteam.points{
	font-size: 40rpx;
	color: #ff6419;
}
.head-iteam.points text{
	font-size: 28rpx;
	color: #999999;
}
.head-iteam.points image{
	position: relative;
	height: 28rpx;
	width: 28rpx;
	margin-left: 2rpx;
	vertical-align: -3rpx;
}
.head-iteam.image .headimg{
	width: 148rpx;
	height: 148rpx;
	border-radius: 100%;
}

.person-nav {
    width: 100%;
    margin: 20rpx 0 0;
    height: 164rpx;
    background-color: #ffffff;
}
.person-nav .items{
  width: 25%;
