  color: #333333
}
.check{
  position: absolute;
  top: 40rpx;
  right: 0;
  width: 180rpx;
	height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
	background-color: #29cccc;
	border-radius: 30rpx 0rpx 0rpx 30rpx;
	opacity: 0.6;
}
.myFavorite_img,.myFavorite_img image{
  width: 100%;
  height: 100%;
}
