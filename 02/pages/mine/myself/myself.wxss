.list-cells {
    position: relative;
    margin-bottom: 20rpx;
    background-color: #fff;
    line-height: 1.41176471;
    font-size: 17rpx;
    padding-left: 30rpx;
}
::-webkit-scrollbar{
  display: none;
}
.list-cells.first,.list-cell.about {
	border: none;
}
.list-cell{
	width: 100%;
  box-sizing: border-box;
	height: 108rpx;
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	color: #403834;
  position: relative;
}
.line{
    position: relative;
    width: 100%;
    bottom: 0;
    background-color: #e8e8e8;
    height: 2rpx;
}
.list-cell__bd,.list-cell__ft{
	height: 108rpx;
	line-height: 108rpx;
}
.list-cell__count{
	height: 15rpx;
  width: 15rpx;
  border-radius: 50%;
  margin: 0 17rpx 0 0;
  position: relative;
  top: 50%;
  margin-top: -7rpx;
  background-color: #f94141;
  color: #fff;
  font-size: 20rpx;
}
.list-cell__bd{
	float: left;
	font-size: 28rpx;
	color: #333333;
}
.list-cell__ft{
	margin-right:40rpx;
	float: right;
	font-size: 24rpx;
	color: #999999;
}
