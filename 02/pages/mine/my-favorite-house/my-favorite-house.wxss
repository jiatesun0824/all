.collect-house{
  padding: 30rpx;
}
.house-item{
  width: 100%;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  background-color: #fff;
  overflow: hidden;
}
.house-img{
  width: 100%;
  height: 518rpx;
  display: block;
}
.house-info{
  position: relative;
  overflow: hidden;
}
.house-title{
  width: 430rpx;
  font-size: 32rpx;
  padding-left: 30rpx;
  margin-top: 30rpx;
  font-weight: bold;
}
.house-type{
  width: 430rpx;
  font-size: 24rpx;
  padding-left: 30rpx;
  margin-top: 23rpx;
  color: #999999;
}
.btn{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 180rpx;
	height: 72rpx;
  font-size: 28rpx;
  border-radius: 36rpx;
  color: #ffffff;
	background-color: #29cccc;
  text-align: center;
  line-height: 72rpx;
}
.operate-box{
  height: 60rpx;
  margin-top: 30rpx;
  padding: 20rpx 30rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  border-top: 1rpx solid #e8e8e8;
  color: #333333;
  display: flex;
  justify-content: flex-start;
}
.toHouseDetail{
  width: 120rpx;
  height: 60rpx;
  margin: 0 0 0 50rpx;
  padding: 0;
  font-size: 24rpx;
  background-color: #fff;
  color: #333333;
  display: flex;
  justify-content: flex-start;
}
button::after{ border: none; }
.operate-iocn{
  width: 60rpx;
	height: 60rpx;
  margin: 0 5rpx 0 0;
}
.operate-iocn-collect{
  width: 48rpx;
	height: 48rpx;
  margin: 6rpx 5rpx 0 0;
}
.icon-box{
  height: 60rpx;
  display: flex;
  justify-content: flex-start;
}
.margin{
  margin-left: 50rpx;
}