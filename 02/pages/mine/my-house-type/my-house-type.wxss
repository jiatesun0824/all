page {
  display: block;
  min-height: 100%;
  background-color: #f5f5f5;
}

.flex-iteam {
  width: 50%;
  height: 100%;
  text-align: center;
  float: left;
  position: relative;
  line-height: 88rpx;
}

.flex-iteam .bayBtn {
  display: inline-block;
  width: 76rpx;
  height: 40rpx;
  font-size: 24rpx;
  color: #fff;
  line-height: 40rpx;
  background-color: #ff6419;
  border-radius: 200rpx;
  margin-left: 10rpx;
}

.flex-iteam::after {
  content: '';
  width: 2rpx;
  height: 24rpx;
  background-color: #e8e8e8;
  right: 0;
  top: 50%;
  margin-top: -12rpx;
  position: absolute;
}

.flex-iteam.iteam-fr::after {
  height: 0;
}

.title {
  font-size: 28rpx;
  color: #999;
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.title .big {
  font-size: 36rpx;
  color: #333;
}

.type-iteam {
  padding: 30rpx 30rpx 0;
  margin-bottom: 20rpx;
  overflow: hidden;
  position: relative;
}

.iteam-mid {
  width: 102rpx;
}

.type-iteam-left {
  width: 100%;
  height: 519rpx;
  position: relative;
  background-color: #ddd;
  border-radius: 10rpx 10rpx 0 0;
}

.type-iteam-left image {
  width: 100%;
  height: 519rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.type-iteam-mid-top {
  font-size: 32rpx;
  width: 310rpx;
  color: #333;
  margin-top: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.type-item-mid-info {
  padding-left: 30rpx;
  border-bottom: 1px solid #e8e8e8;
}

.type-item-mid-link {
  height: 101rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #333;
  padding-left: 30rpx;
}

.type-iteam-mid-share {
  margin-right: 50rpx;
}

.type-iteam-mid-share .share-button {
  display: flex;
  align-items: center;
  border: none;
  background: #fff;
  font-size: 24rpx;
  color: #333;
  padding: 0
}
.share-button::after{
  border: none;
}
.type-iteam-mid-share .share-logo {
  width: 60rpx;
  height: 60rpx;
  margin-right: 14rpx;
}

.type-iteam-mid-publish-news {
  display: flex;
  align-items: center;
}

.type-iteam-mid-publish-news .publish-news-logo {
  width: 60rpx;
  height: 60rpx;
  margin-right: 17rpx;
}

.type-iteam-mid-mid {
  margin: 19rpx 0;
  overflow: hidden;
  width: 310rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.type-iteam-mid {
  height: 240rpx;
  position: relative;
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
  background: #fff;
  border-radius: 0 0 10rpx 10rpx;
}

.type-iteam-no-mid {
  width: 500rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
}

.type-iteam-no-mid-top {
  font-size: 36rpx;
  color: #333;
  font-weight: 600;
}

.type-iteam-right {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}

.min-btn {
  text-align: center;
  font-size: 28rpx;
  width: 180rpx;
  height: 72rpx;
  line-height: 72rpx;
  border-radius: 36rpx;
  color: #fff;
  background: #29cccc;
}

.btn {
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  bottom: 30rpx;
  left: 50%;
  width: 690rpx;
  height: 88rpx;
  margin-left: -345rpx;
  text-align: center;
  line-height: 88rpx;
  background-color: #ff6419;
  border-radius: 44rpx;
}

.img {
  width: 220rpx;
  height: 183rpx;
}

.buy-house-btn {
  width: 100%;
  height: 80rpx;
  position: fixed;
  left: 0;
  bottom: 30rpx;
}

.house-btn {
  width: 690rpx;
  height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #fff;
  margin: 0 auto;
  background-color: #ff6419;
}

.systemInfo-btn {
  width: 280rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-top: 80rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  background-color: #ff6419;
  border-radius: 40rpx;
  text-align: center;
}

.empty_tip {
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
  margin-top: -100rpx;
  top: 0;
}

.empty_tip .image {
  width: 600rpx;
  height: 450rpx;
}

.empty_tip .txt {
  font-size: 28rpx;
  color: #999;
  margin-top: 40rpx;
}

.uncontentlist .type-iteam-mid view {
  min-height: 20rpx;
}

.no-btn {
  margin-top: 40rpx;
  margin-left:160rpx;

}

/** 修改button默认的点击态样式类**/

.button-hover {
  background-color: #fff;
  border: none;
}

.padding{
  padding-bottom: 30rpx
}