/* rzd 户型引导弹框开始 */

.boot_sucAertbg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 110000000;
}

.boot_alert {
  width: 540rpx;
  height: 540rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  margin-left: -270rpx;
  top: 45%;
  margin-top: -270rpx;
  z-index: 1100000001;
}

.close_boot {
  height: 66rpx;
  padding-right: 20rpx;
  text-align: right;
  padding-top: 20rpx;
}

.close_boot image {
  width: 40rpx;
  height: 40rpx;
}

.boot_sucicon {
  width: 184rpx;
  height: 184rpx;
  display: block;
  margin: 0 auto;
}

.boot_title {
  text-align: center;
  font-size: 36rpx;
  color: #29cccc;
}

.boot_tit {
  text-align: center;
  font-size: 24rpx;
  margin-top: 29rpx;
  color: #999;
}

.boot_backbut {
  width: 400rpx;
  height: 80rpx;
  background-color: #29cccc;
  border-radius: 40rpx;
  margin: 0 auto;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  margin-top: 47rpx;
  color: #fff;
}

/* rzd 户型引导弹框结束 */

view {
  box-sizing: border-box;
}

.upload-input {
  color: #999;
  font-size: 28rpx;
  z-index: 0;
}

.upload-house {
  width: 100%;
  height: 100%;
  background-color: #fff;
  margin-top: 20rpx;
  position: relative;
}

.upload-house .house-title {
  width: 750rpx;
  height: 24rpx;
  color: #999;
  font-size: 24rpx;
  line-height: 24rpx;
  text-align: center;
  margin-top: 47rpx;
  margin-bottom: 30rpx;
}

/* 户型主要信息模块 */

.upload-house .house-address {
  padding-left: 30rpx;
  line-height: 88rpx;
}

.upload-house .house-address text {
  color: #333;
  font-size: 28rpx;
  margin-right: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  position: relative;
}

.upload-house .house-address text text {
  margin: 0 !important;
  padding: 0 !important;
  display: block;
  width: 30rpx;
  height: 100%;
  line-height: 88rpx;
  position: absolute;
  left: 0;
  top: 0;
  color: #29cccc;
  font-size: 30rpx;
}

.upload-house .house-address input {
  width: 549rpx;
  height: 88rpx;
  font-size: 28rpx;
  position: relative;
  z-index: 3;
}

.house-address .common {
  border-bottom: solid #e8e8e8 1rpx;
}

/* 户型上传图片模块 */

.upload-house .house-message {
  padding: 40rpx 40rpx 0 40rpx;
}

.upload-house .message-pictrue {
  position: relative;
  width: 670rpx;
  height: 503rpx;
  background-color: #f5f5f5;
  border-radius: 10rpx;
}

