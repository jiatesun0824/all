/* 我的方案顶部tab栏 B */
.navTop{
  width: 100%;
	height: 88rpx;
  position: fixed;
  top: 0;
  left: 0;
	background-color: #ffffff;
  box-sizing: border-box;
  z-index: 330;
}

.navTop scroll-view{
  width: 100%;
  height: 88rpx;
  position: absolute;
  left: 0;
  z-index: 34;
  box-sizing: border-box;
  padding: 4rpx 0 4rpx 60rpx;
  white-space: nowrap;
}
.navTop scroll-view .items{
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: relative;
  color: #000000;
  font-size: 28rpx;
  margin-right: 62rpx;
}
.navTop scroll-view .items.active{
  color: #333333;
  font-weight: bold;
}
.navTop scroll-view .items.active::after{
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  margin-left: -20rpx;
  width: 40rpx;
	height: 6rpx;
	background-color:#29cccc;
	border-radius: 3px;
}
