/* pages/putInMyHouse/putInMyHouse.wxss */
page{
  background-color: #fff;
}
.coin{
  display: flex;
  flex-direction: row;
  margin-top: 40rpx;
  width: 690rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.coin .left{
  width: 345rpx;
  font-size: 28rpx;
  color: #333333;
  text-align: left;
  line-height: 30rpx;
}
.coin .right{
  width: 345rpx;
  font-size: 24rpx;
  color: #999999;
  text-align: right;
  line-height: 30rpx;
}
.option{
  height: 160rpx;
  width: 690rpx;
  border: solid 1rpx #ff6419;
  margin-left: 30rpx;
  margin-top: 40rpx;
  border-radius: 6rpx;
   font-size: 28rpx;
  color: #ff6419;
  text-align: center;
  display: flex;
  flex-direction: row;
}
.option .left{
  width: 345rpx;
  line-height: 160rpx;
 
  
}
.option .right{
  width: 345rpx;
  line-height: 160rpx;
  font-weight: bold;
}
.button{
  width: 690rpx;
	height: 88rpx;
	background-color: #ff6419;
	border-radius: 44rpx;
  margin-left: 30rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #ffffff;
  position: fixed;
  bottom: 30rpx;
}
.integral-insufficient{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}