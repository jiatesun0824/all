/* pages/plan/selection-scheme/selection-scheme.wxml.wxss */

.selections-scheme {
  width: 100%;
  height: 100%;
}
page{
  background-color: #fff;
}
/* header B */

.header {
  width: 100%;
  height: 90rpx;
  position: fixed;
  padding: 0 30rpx;
  top: 0;
  left: 0;
  z-index: 6;
  background-color: #fff;
  white-space: nowrap;
  overflow-x: auto;
}
.header>view:last-child{
  margin-right: 30rpx;
}

.header::-webkit-scrollbar {
  display: none;
}

.header_title {
  line-height: 90rpx;
  font-size: 32rpx;
  color: #323232;
  padding: 0 30rpx;
  display: inline-block;
  position: relative;
}

.header_title .txt {
  color: #333;
  margin: 0;
}

.header_active .txt {
  color: #333;
  font-weight: 600;
  margin: 0;
}

.header_active:after {
  content: "";
  position: absolute;
  bottom: 4rpx;
  height: 6rpx;
  left: 50%;
  margin-left: -25rpx;
  background-color: #29cccc;
  display: block;
  width: 40rpx;
  border-radius: 3px;
}

.header_xian {
  height: 32rpx;
  width: 2rpx;
  background: #e8e8e8;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -16rpx;
}

/* header E */

/* 收藏方案tab栏切换 B */

.collectPlanTop {
  height: 88rpx;
  width: 750rpx;
  background-color: #fff;
  position: fixed;
  top: 0;
  left: 0;
  border-top: 1rpx solid #efefef;
  color: #333;
  font-size: 28rpx;
  z-index: 30;
  margin-top: 90rpx;
}

.collectPlanTop .active {
  color: #29cccc;
}

.collectPlanTop image {
  width: 16rpx;
  height: 16rpx;
}

.space_type {
  height: 88rpx;
  width: 250rpx;
  text-align: center;
  line-height: 90rpx;
}

.space_area {
  height: 90rpx;
  width: 250rpx;
  text-align: center;
  line-height: 90rpx;
}

.space_style {
  height: 90rpx;
  width: 250rpx;
  text-align: center;
  line-height: 90rpx;
}

.space {
  height: 90rpx;
}

/* 收藏方案tab栏切换 E */

/* 收藏方案内容栏 B */

.case-condition {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 179rpx;
  border-top: 1rpx solid #efefef;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 10;
}

.condition-box {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background-color: #fff;
  padding: 31rpx 0rpx 8rpx 31rpx;
  border-top: 1rpx solid #e8e8e8;
  box-sizing: border-box;
  overflow: auto;
}

.condition-box .single-box {
  width: 216rpx;
  height: 80rpx;
  background-color: #f5f5f5;
  border-radius: 5rpx;
  margin: 0 20rpx 20rpx 0;
  text-align: center;
  font-size: 28rpx;
  line-height: 80rpx;
  color: #333;
}

.condition-box .single-box.condition-box-active {
  background-color: #29cccc;
  color: #fff;
}

/* 收藏方案内容栏 E */

/* 收藏列表 B */

.list {
  height: auto;
  padding: 30rpx 30rpx 120rpx;
}

.listTop {
  height: auto;
  margin-top: 180rpx;
  padding: 30rpx 30rpx 120rpx;
}

.list-item {
  background-color: #fff;
  height: auto;
  overflow: hidden;
  display: flex;
}

.item-img {
  width: 560rpx;
  height: 315rpx;
  position: relative;
}

.operate {
  position: absolute;
  top: 0;
  right: 0;
  width: 68rpx;
  height: 44rpx;
  background: rgba(17, 17, 17, 0.5);
  border-radius: 0rpx 10rpx 0rpx 20rpx;
  color: #fff;
  text-align: center;
  line-height: 22rpx;
  letter-spacing: 5rpx;
}

.operate-box {
  position: absolute;
  top: 70rpx;
  right: 20rpx;
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 6px;
  z-index: 11;
}

.operate-box .options {
  height: 80rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  line-height: 80rpx;
}

.item-img .btn {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 100rpx;
  height: 100rpx;
  opacity: 0.9;
}

.item-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.checkBtn {
  width: 56rpx;
  height: 56rpx;
  text-align: center;
  margin-top: 129rpx;
  margin-left: 20rpx;
}

.bottom-box {
  height: 98rpx;
  line-height: 98rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.left .name {
  width: 260rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.left .date {
  width: auto;
  font-size: 24rpx;
  color: #999;
}

.bottomBtn {
  display: flex;
  margin-top: 50rpx;
}

.price-btn {
  width: 148rpx;
  height: 60rpx;
  margin: 10rpx 0 0 0rpx;
  border-radius: 6rpx;
  border: solid 1rpx #ff8c19;
  font-size: 28rpx;
  color: #ff8c19;
  line-height: 60rpx;
  text-align: center;
}

.continue-btn {
  width: 148rpx;
  height: 60rpx;
  margin: 10rpx 0 0 16rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 6rpx;
  color: #29cccc;
  border: solid 1rpx #29cccc;
}

.shareBtn {
  width: 148rpx;
  height: 60rpx;
  margin: 10rpx 0 0 16rpx;
  border-radius: 6rpx;
  font-size: 28rpx;
  color: #29cccc;
  line-height: 60rpx;
  text-align: center;
  background-color: #fff;
  border: solid 1rpx #29cccc;
  box-sizing: inherit;
  padding: 0;
}

/* 收藏列表 E */

/* 确定按钮 B*/

.confirm {
  width: 100%;
  height: 120rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  padding: 20rpx 30px;
  box-sizing: border-box;
}

.confirmBtn {
  width: 100%;
  height: 80rpx;
  display: inline-block;
  background-color: #29cccc;
  border-radius: 40px;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}
.plan{
  margin-left: 50rpx;
}

.grayLine{
  width:750rpx;
height:2rpx;
background-color:#e8e8e8;
margin-bottom:40rpx;
margin-left:-30rpx;
}
/* 确定按钮 E*/
