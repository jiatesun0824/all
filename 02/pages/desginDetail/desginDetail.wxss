/* pages/desginDetail/desginDetail.wxss */

page {
  background-color: #fff;
}

.title {
  width: 690rpx;
  font-size: 48rpx;
  color: #333;
  font-weight: 600;
  margin-left: 30rpx;
  margin-top: 40rpx;
}

.author {
  width: 690rpx;
  height: 68rpx;
  margin: 60rpx 30rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
}

.head {
  display: flex;
}

.headPic {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  background-color: #e8e8e8;
}

.name {
  margin-left: 15rpx;
}

.nick {
  font-size: 28rpx;
  color: #333;
  width: 400rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
}

.time {
  font-size: 20rpx;
  color: #999;
  margin-top: 5rpx;
}

.unitly {
  width: 50rpx;
  height: 50rpx;
  margin-left: 25rpx;
}

.message {
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
  margin-bottom: 35rpx;
  margin-top: 40rpx;
  width: 690rpx;
  margin-left: 30rpx;
  font-weight: 600;
}

.message-img {
  width: 690rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  box-shadow: 0px 10px 30px 0px rgba(232, 232, 232, 0.4);
}

.gary {
  width: 750rpx;
  height: 20rpx;
  background-color: #f5f5f5;
}

.houseTypeImg {
  width: 690rpx;
  height: 521rpx;
  border-radius: 8rpx;
  margin-left: 30rpx;
  margin-top: 60rpx;
}

.houseInfo {
  width: 690rpx;
  height: 160rpx;
  background-color: #fff;
  box-shadow: 0rpx 5rpx 40rpx 0rpx rgba(204, 204, 204, 0.4);
  border-radius: 6rpx;
  margin-left: 30rpx;
  margin-top: 60rpx;
  display: flex;
}

.houseInfo .info {
  width: 172rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tit {
  color: #999;
  font-size: 28rpx;
  margin-top: 30rpx;
}

.data {
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
}

.comment .top {
  height: 90rpx;
  width: 690rpx;
  background-color: #fff;
  padding: 0rpx 30rpx;
  justify-content: cneter;
  line-height: 90rpx;
  border-bottom: 1rpx solid #e8e8e8;
}

.comment .top .left {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.comment .top .right {
  font-size: 28rpx;
  color: #999;
}

.comment .top image {
  width: 14rpx;
  height: 8rpx;
  vertical-align: middle;
}

.content {
  width: 690rpx;
  margin-left: 30rpx;
  padding: 40rpx 0 40rpx 0;
}

.content .info {
  /* height: 68rpx; */
  width: 690rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}

.content .info .personal {
  display: flex;
  flex: 6;
}

.content .info .personal .head {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}

.content .info .personal .goStore {
  position: absolute;
  bottom: -25rpx;
  left: -3rpx;
  width: 76rpx;
  height: 32rpx;
  text-align: center;
  background-color: #29cccc;
  border-radius: 4rpx;
  font-size: 22rpx;
  color: #fff;
  z-index: 20;
}

.content .info .personal .other {
  /* height: 68rpx; */
  margin-left: 20rpx;
}

.other .username {
  color: #333;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 28rpx;
  font-weight: bold;
  display: inline-block;
  vertical-align: text-top;
  margin-top: 10rpx;
}

.other .userType {
  line-height: 20rpx;
  font-size: 24rpx;
  margin-left: 12rpx;
  border-radius: 4rpx;
  display: inline-block;
  color: #29cccc;
  padding: 5rpx 9rpx;
  box-sizing: border-box;
  border: solid 1rpx #29cccc;
  vertical-align: text-top;
  margin-top: 10rpx;
}

.other .topMessageStyle {
  width: 60rpx;
  height: 32rpx;
  display: inline-block;
  text-align: center;
  background-color: #fa3;
  border-radius: 4rpx;
  border: solid 1rpx #fa3;
  font-size: 22rpx;
  margin-left: 12rpx;
  color: #fff;
  vertical-align: text-top;
  margin-top: 10rpx;
}

.content .data {
  color: #999;
  font-size: 22rpx;
  display: inline-block;
  margin: 30rpx 0rpx 48rpx 88rpx;
}

.content .topMessage, .content .deleteBtn {
  color: #999;
  font-size: 22rpx;
  display: inline-block;
  margin-left: 5rpx;
}

.deleteBtn .deleteImg {
  width: 60rpx;
  height: 60rpx;
  text-align: center;
}

.noComment {
  height: 360rpx;
  text-align: center;
  line-height: 360rpx;
  color: #999;
  font-size: 28rpx;
}

.like {
  /* height: 68rpx; */
  display: flex;
}

.like image {
  height: 48rpx;
  width: 48rpx;
  vertical-align: middle;
}

.like .txt, .pageview-box-right .txt {
  font-size: 24rpx;
  margin-left: 5rpx;
  color: #333;
  margin-top: 11rpx;
}

.like .txtActive, .pageview-box-right .txtActive {
  font-size: 24rpx;
  margin-left: 5rpx;
  line-height: 48rpx;
  color: #fa3;
}

.content .textCot {
  width: 602rpx;
  display: block;
  margin-left: 88rpx;
  word-wrap: break-word;
  font-size: 28rpx;
  margin-top: 25rpx;
}

.reply {
  min-height: 98rpx;
  height: auto;
  width: 750rpx;
  background-color: #fff;
  position: fixed;
  display: flex;
  align-items: center;
  border-top: 1rpx solid #e8e8e8;
  z-index: 99
}

.replyTxt {
  background-color: #f5f5f5;
  width: 440rpx;
  height: 64rpx;
  border-radius: 31rpx;
  margin-left: 30rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  color: #999;
  text-align: left;
  padding-left: 20rpx;
}

.reply .untily {
  position: relative;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
}

.reply .replyIcon {
  width: 56rpx;
  height: 56rpx;
}

.replyTxt2 {
  width: 650rpx;
  max-height: 192rpx;
  border-radius: 31rpx;
  /* padding: 12rpx 30rpx; */
  font-size: 28rpx;
  padding:15rpx 0 15rpx 30rpx;

}

.submit {
  width: 148rpx;
  height: 64rpx;
  background-color: #29cccc;
  border-radius: 32rpx;
  line-height: 64rpx;
  text-align: center;
  margin-left: 20rpx;
  color: #fff;
  font-size: 28rpx;
  margin-right: 30rpx;
}

.list {
  /* height: 640rpx; */
  width: 690rpx;
  margin-top: 20rpx;
  /* border: 1rpx solid #e8e8e8; */
  border-radius: 10rpx;
  box-sizing: border-box;
  box-shadow: 0px 10px 30px 0px rgba(232, 232, 232, 0.4);
  margin-left: 30rpx;
  margin-bottom: 30rpx;
}

.scroll {
  margin-top: 40rpx;
}

.listView {
  display: flex;
  flex-direction: row;
}

.list .top {
  width: 690rpx;
  height: 519rpx;
  background-color: #e0e0e0;
  border: 1rpx solid #e8e8e8;
  border-radius: 10rpx;
}

.list .top image {
  width: 690rpx;
  height: 518rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.list .top2 {
  width: 690rpx;
  height: 388rpx;
  position: relative;
}

.list .top2 image {
  width: 690rpx;
  height: 388rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.list .top2 .tag {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 80rpx;
  height: 80rpx;
  /* opacity: 0.9; */
}

.list .bottom {
  width: 690rpx;
  height: 148rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
  display: flex;
  flex-direction: row;
  border: 1rpx solid #e8e8e8;
  border-top: 0;
}

.list .bottom .left {
  height: 148rpx;
  width: 280rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  display: flex;
  flex-direction: column;
}

.list .bottom .left .tit {
  font-size: 32rpx;
  color: #403834;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 27rpx;
  font-weight: 600;
}

.list .bottom .left .area {
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 15rpx;
}

.list .bottom .right {
  height: 148rpx;
  width: 320rpx;
}

.toHouseDetail {
  width: 180rpx;
  height: 72rpx;
  background-color: #29cccc;
  border-radius: 36rpx;
  font-size: 28rpx;
  color: #fff;
  margin-top: 33rpx;
  text-align: center;
  line-height: 72rpx;
}

.mask-pop {
  background: rgba(0, 0, 0, 0.6);
  z-index: 999;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}

.cancel {
  color: #000;
  font-weight: 700;
}

.choose-box {
  border-bottom: 4rpx solid #e8e8e8;
}

.pop-box {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 100;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: #333;
}

.share-btn {
  width: 56rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: none;
  position: relative;
  border: none;
  outline: none;
}

.share-btn:after {
  border: none;
}

.share-btn image {
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 0;
  left: 0;
  margin-left: 0;
}

.txtBack {
  background-color: #f5f5f5;
  width: 690rpx;
  height: auto;
  border-radius: 31rpx;
  /* line-height: 64rpx; */
  /* margin: 17rpx auto; */
  display: flex;
  flex-direction: column;
  justify-content:center;
}

.txtBack input {
  display: flex;
  align-items: center;
}

.num {
  width: 40rpx;
  height: 24rpx;
  background-color: #fa3;
  border-radius: 12rpx;
  line-height: 24rpx;
  font-size: 20rpx;
  color: #fff;
  position: absolute;
  right: -13rpx;
  top: 8rpx;
  text-align: center;
}

.block {
  height: 99rpx;
}

.add {
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  display: flex;
  justify-content: space-between;
}

.add .btn {
  margin-left: 30rpx;
  display: flex;
}

.add image {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
}

.add .addplan {
  width: 60rpx;
  height: 35rpx;
  font-size: 20rpx;
  border: solid 2rpx #666;
  line-height: 35rpx;
  border-radius: 10rpx;
  text-align: center;
  margin-top: 12rpx;
  margin-left: 30rpx;
}

.txtBack .img {
  border-bottom: 1rpx solid #e8e8e8;
  padding: 20rpx 20rpx 10rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
}

.txtBack .img .imag {
  height: 190rpx;
  width: 190rpx;
  margin-bottom: 10rpx;
  margin-right: 25rpx;
}

.txtBack .img .delete {
  position: absolute;
  right: 0;
  top: -13rpx;
  width: 40rpx;
  height: 40rpx;
}

.txtBack .img .tag {
  position: absolute;
  background-color: #29cccc;
  left: 0;
  top: 0;
  width: 60rpx;
  height: 35rpx;
  font-size: 20rpx;
  color: #fff;
  line-height: 35rpx;
  text-align: center;
  border-radius: 5rpx;
}

.commentImg {
  margin-top: 20rpx;
  display: flex;
  margin-left: 88rpx;
  padding-bottom: 10rpx;
}

.commentImg image {
  width: 198rpx;
  height: 198rpx;
  margin-right: 30rpx;
  border-radius: 6rpx;
}

.plan {
  margin-left: 88rpx;
  margin-top: 30rpx;
  position: relative;
}

.content .plan .image {
  width: 600rpx;
  height: 450rpx;
  border-radius: 6rpx;
}

.content .tag {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 80rpx;
  height: 80rpx;
}

.scroll {
  height: 2000rpx;
}

.placeholder {
  font-size: 28rpx;
  line-height: 64rpx;
}
.placeholder-class{
  display: flex;
  align-items: center;
  
}
.garyLine{
  height:2rpx;
  width:662rpx;
  background-color:#e8e8e8;
  margin-left:88rpx;
}
.content:last-child .garyLine{
  background-color:#fff;
