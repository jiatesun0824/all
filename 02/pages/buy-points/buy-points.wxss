page {
    display: block;
    min-height: 100%;
    background-color: #FFFFFF;
}
.block{
	
	background: #fff;
	overflow: hidden;
}
.block-top{
	padding: 0 32rpx;
	position: relative;
}
.words{
	margin-top: 66rpx;
	width: 100%;
	color: #333333;
	font-size: 28rpx;
}
.nums{
	width: 100%;
	margin-top: 31rpx;
	margin-bottom:61rpx;
	font-size: 126rpx;
	color: #ff6419;
}
.nums text{
	font-size: 28rpx;
	color: #999999;
	
}
.bg{
	position: absolute;
	top: 70rpx;
	right: 0rpx;
	width: 172rpx;
	height: 146rpx;
	z-index: 1;
}
.block_list{
	width: 100%;
}
.block_list_iteam{
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
	width: 100%;
	font-size: 28rpx;
	height: 88rpx;
	line-height: 88rpx;
	color: #333333;
	border-bottom:1px solid #ddd;
}
.block_list_iteam:last-child{
	border: none;
}
.block_list .left{
	margin-left: 31rpx;
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.block_list .right{
	overflow: hidden;
	margin-right: 31rpx;
	height: 50rpx;
	width: 50rpx;
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;

}
.block-check-type{
	height: 50rpx;
	width: 50rpx;
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.block-check{
	position:absolute;
    left:-9999px;

}
.check-bg{
	margin:0 auto;
	width: 41rpx;
	height: 41rpx
	
}

.block-btn{
	
	width: 100%;
}

.block-gird{

}
.block-points{
	height:88rpx;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #333333;
	padding:0 32rpx;
	background: #fff;

}
.block-points-each{
	padding: 28rpx 32rpx;
	color: #999999;
	font-size: 28rpx;
}
.block-points .fr{
	color: #999999;
}

.btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: 50rpx;
    width: 690rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    text-align: center;
    text-decoration: none;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 39rpx;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #FFFFFF;
    background-color: #ff6419;
}
button[type=primary] {
    color: #FFFFFF;
    background-color: #ff6419;
}

.block-gird-iteam{
	margin:0 0rpx 30rpx 24rpx;
	width: 216rpx;
	height: 140rpx;
	background: #fff;
	border: 1px solid #e0e0e0;
	border-radius: 5rpx;
	text-align: center;
  position:relative;
}
.block-gird-iteam-active{

  margin:0 0rpx 30rpx 24rpx;
	width: 216rpx;
	height: 140rpx;
	background: #fff;
	border: 1px solid #ff6419;
	border-radius: 5rpx;
	text-align: center;
	overflow: hidden;
  position:relative;
}


.block-gird-iteam .top{
	font-weight: 600;
	margin-top: 27rpx;
	font-size: 32rpx;
	color: #333333;
}
.block-gird-iteam-active .top{
	font-weight: 600;
	margin-top: 27rpx;
	font-size: 32rpx;
	color: #ff6419;
}
.block-gird-iteam-active .bottom{
	font-weight: 400;
	margin-top:5rpx;
	margin-bottom: 30rpx;
	font-size: 32rpx;
	color: #ff6419;
}
.block-gird-iteam .bottom{
	font-weight: 400;
	margin-top:5rpx;
	margin-bottom: 30rpx;
	font-size: 32rpx;
	color: #999;
}
.notice{
	margin:0 auto;
	width: 590rpx;
	margin-top: 30rpx;
	font-size: 24rpx;
	line-height:1.8;
	color: #999999
}

.discount{
  width: 70rpx;
  height: 36rpx;
  position:absolute;
