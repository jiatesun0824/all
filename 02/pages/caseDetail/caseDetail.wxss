
.title{
  margin-top:30rpx;
  margin-left: 30rpx; 
  font-size: 48rpx;
  color: #494949;
  font-weight: 550;
  width: 690rpx;
  word-break: break-all;
}
.information{
  font-size: 24rpx;
  color: #999999;
  display: flex;
  flex-direction: row
}
.shopname{
  margin-left: 30rpx;
  margin-top: 30rpx;
}
.time{
  margin-left: 15rpx;
  margin-top: 30rpx;
}
.content{
  margin-top:40rpx;
  width: 100%rpx;
  margin-bottom:50rpx; 
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
}
.content view{
    width: 100%;
    text-align: left;
    box-sizing: border-box;
}
.content view.ql-align-center{
    text-align: center;
}
.content view.ql-align-right{
    text-align: right;
}
.teamimage{
  width: 690rpx;
  margin-bottom: 30rpx;
  border-radius: 6rpx;
}
.contenttxt{
 width: 690rpx;
 font-size: 28rpx; 
 line-height: 48rpx;
 margin-bottom: 30rpx;
 color: #333333;
 
}