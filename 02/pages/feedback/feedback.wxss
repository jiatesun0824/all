page{
    background: #fff;
    width: 100%;
    min-height: 100%;
    position: relative;
    padding: 0 30rpx;
    overflow-y: scroll;
    box-sizing: border-box;
}
.titles{
    padding: 40rpx 0 30rpx;
    width: 100%;
    line-height: 1;
    font-weight: 600;
    font-size: 32rpx;
    box-sizing: border-box;
    color: #000;
}
.ttMain{
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx;
    padding-bottom: 70rpx;
    height: 400rpx;
    border-radius: 6rpx;
    background: #f5f5f5;
    position: relative;
}
.ttMain textarea{
    font-size: 26rpx;
    color: #333;
    height: 330rpx;
    line-height: 1.4;
    width: 100%;
}
.ttMain .txts{
    position: absolute;
    text-align: right;
    font-size: 24rpx;
    color: #999;
    right: 30rpx;
    bottom: 48rpx;
    line-height: 1;
}
input.phone{
    width: 100%;
    box-sizing: border-box;
    padding: 0 30rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 26rpx;
    color: #333;
    margin-bottom: 60rpx;
    border-radius: 6rpx;
    background: #f5f5f5;
}
.btns{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    border-radius: 100rpx;
    background: #29cccc;
}
.toList{
    text-align: center;
    font-size: 28rpx;
    line-height: 1;
    left: 0;
    width: 100%;
    margin: 40rpx auto;
}
.mainUpImg{
    width: 100%;
    height: 200rpx;
    box-sizing: border-box;
    padding: 30rpx;
    background-color: #f5f5f5;
	border-radius: 6rpx;
}
.mainUpImg>view{
    float: left;
    margin-right: 20rpx;
    width: 140rpx;
    height: 140rpx;
    position: relative;
}
.mainUpImg>view:last-child{
    margin: 0;
}
.mainUpImg>view image.imgs{
    display: block;
    width: 140rpx;
    height: 140rpx;
    border-radius: 6rpx;
}
.mainUpImg>view image.close{
    position: absolute;
    width: 44rpx;
    height: 44rpx;
    top: -10rpx;
    right: -10rpx;
    background: rgba(0,0,0,.45);
    border-radius: 50%;
}
.upImg{
    display: block;
    float: left;
    width: 140rpx;
    height: 140rpx;
    border-radius: 6rpx;
    box-shadow: 0 10rpx 30rpx 0 
		rgba(245, 245, 245, 0.8);
}
.toast{
    width: 340rpx;
	height: 280rpx;
	background-color: rgba(0,0,0,.7);
	border-radius: 10rpx;
    position: fixed;
    top: 348rpx;
    left: 50%;
    box-sizing: border-box;
    padding: 48rpx 0;
    margin-left: -170rpx;
}
.toast image{
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    top: 48rpx;
    left: 50%;
    margin-left: -44rpx;
}
.toast view{
    width: 100%;
    text-align: center;
    position: absolute;
    left: 0;
    color: #fff;
    font-size: 26rpx;
    line-height: 1;
}
.toast view.p1{
    top: 166rpx;
}
.toast view.p2{
    bottom: 47rpx;
}