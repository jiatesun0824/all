button[class="submit"]{
  width: 148rpx;
	height: 64rpx;
	border-radius: 32rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 64rpx;
  text-align: center;
  background-color: #29cccc;
  margin-left: 20rpx;
}

.mask-pop {
  background: rgba(0, 0, 0, 0.6);
  z-index: 999;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}

.cancel {
  color: #000;
  font-weight: 700;
}

.choose-box {
  border-bottom: 4rpx solid #e8e8e8;
}
.pop-box {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 100;
  text-align: center;
  line-height: 100rpx;
  font-size: 34rpx;
  color: #333;
}

.banner .tag{
  position: absolute;
  top:171rpx;
  left:335rpx;
  width: 80rpx;
  height: 80rpx;
}


.share-btn {
  width: 56rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: none;
  position: relative;
  border: none;
  outline: none;
}

.share-btn:after {
  border: none;
}

.share-btn image {
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 0;
  left: 0;
  margin-left: 0;
}
.txtBack{
  background-color:#f5f5f5;
width:522rpx;
height:64rpx;
border-radius:31rpx;
margin-left:30rpx;
line-height:64rpx;
}
.num{
  width: 40rpx;
	height: 24rpx;
	background-color: #ffaa33;
	border-radius: 12rpx;
  line-height: 24rpx;
  font-size: 20rpx;
  color:#fff;
  position: absolute;
  right:-13rpx;
  top:8rpx;
  text-align: center;
}
.placeholder{
  font-size: 28rpx;
  line-height: 64rpx;
  /* margin-top: 20rpx; */
}
.garyLine{
  height:2rpx;
width:662rpx;
background-color:#e8e8e8;
