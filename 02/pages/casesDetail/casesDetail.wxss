/* pages/casesDetail/casesDetail.wxss */
page{
  background-color: #fff;
}
.top{
  position: relative
}
.banner{
  width: 750rpx;
	height: 422rpx;
}
.bannerImg{
  width: 750rpx;
	height: 422rpx;
}
.imgNum{
  width: 88rpx;
	height: 40rpx;
	background-color: #000000;
	border-radius: 20rpx;
	opacity: 0.6;
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  color: #fff;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
}
.case{
  width: 690rpx;
  margin-left: 30rpx;
}
.title{
  width: 690rpx;
  font-size: 48rpx;
  color: #333333;
  font-weight: 600;
  margin-left: 30rpx;
  margin-top: 40rpx;
}
.author{
  width: 690rpx;
  height: 68rpx;
  margin:60rpx 30rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
}
.head{
  display: flex;
}
.headPic{
  width: 68rpx;
	height: 68rpx;
  border-radius: 50%;
  background-color: #e8e8e8;
}
.name{
  margin-left: 15rpx;
}
.nick{
  font-size: 28rpx;
  color: #333;
  width: 400rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
}
.time{
  font-size: 20rpx;
  color: #999999;
  margin-top: 5rpx;
}
.unitly{
  width: 50rpx;
	height: 50rpx;
  margin-left: 25rpx;
}
.message{
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333333;
  margin-bottom: 35rpx;
  margin-top: 40rpx;
  width: 690rpx;
  margin-left: 30rpx;
}
.message-img{
  width: 690rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  box-shadow: 0px 10px 30px 0px 
		rgba(232, 232, 232, 0.4);
}
.gary{
  width: 750rpx;
  height: 20rpx;
  background-color: #f5f5f5;
}
.houseTypeImg{
  width: 690rpx;
	height: 521rpx;
  border-radius: 8rpx;
  margin-left: 30rpx;
  margin-top: 60rpx;
}
.houseInfo{
  width: 690rpx;
	height: 160rpx;
	background-color: #ffffff;
	box-shadow: 0rpx 5rpx 40rpx 0rpx 
		rgba(204, 204, 204, 0.4);
	border-radius: 6rpx;
  margin-left: 30rpx;
  margin-top: 60rpx;
  display: flex
}
.houseInfo .info{
  width: 172rpx;
  display:flex;
  flex-direction:column;
  align-items:center;

}
.tit{
  color: #999999;
  font-size: 28rpx;
  margin-top: 30rpx;
}
.data{
  font-size: 28rpx;
  color: #333333;
  margin-top: 20rpx;
  font-weight:600;

}



.comment .top{
  height: 90rpx;
  width: 690rpx;
  background-color: #fff;
  padding:0rpx 30rpx;
  justify-content: cneter;
  line-height: 90rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.comment .top .left{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.comment .top .right{
  font-size: 28rpx;
  color: #999999;
}
.comment .top image{
  width: 14rpx;
  height: 8rpx;
  vertical-align: middle
}
.content {
  width: 690rpx;
  margin-left: 30rpx;
  padding: 40rpx 0 0;
}
.content .info{
  /* height: 68rpx; */
  width: 690rpx;
  display: flex;
  justify-content:space-between;
  position: relative
}
.content .info .personal{
  display: flex;
  flex: 6
}
.content .info .personal .head{
  width: 68rpx;
	height: 68rpx;
  border-radius: 50%;
}
.content .info .personal .goStore{
  position: absolute;
  bottom: -25rpx;
  left: -3rpx;
  width: 76rpx;
	height: 32rpx;
  text-align: center;
	background-color: #29cccc;
	border-radius: 4rpx;
  font-size: 22rpx;
  color: #ffffff;
  z-index: 20
}
.content .info .personal .other{
  /* height: 68rpx; */
  margin-left: 20rpx;
}
.other .username{
  color: #333333;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 28rpx;
  font-weight: bold;
  display: inline-block;
  vertical-align: text-top;
  margin-top: 10rpx;
}
.other .userType{
  line-height: 20rpx;
  font-size: 24rpx;
  margin-left: 12rpx;
	border-radius: 4rpx;
  display: inline-block;
  color:#29cccc;
  padding: 5rpx 9rpx;
  box-sizing: border-box;
	border: solid 1rpx #29cccc;
  vertical-align: text-top;
  margin-top: 10rpx;
}
.other .topMessageStyle{
  width: 60rpx;
	height: 32rpx;
  display: inline-block;
  text-align: center;
	background-color: #ffaa33;
	border-radius: 4rpx;
	border: solid 1rpx #ffaa33;
  font-size: 22rpx;
  margin-left: 12rpx;
	color: #ffffff;
  vertical-align: text-top;
  margin-top: 10rpx;
}
.content .data{
  color: #999999;
  font-size: 22rpx;
  display: inline-block;
  margin: 30rpx 0rpx 48rpx 88rpx;
}
.content .topMessage,
.content .deleteBtn{
  color: #999;
  font-size: 22rpx;
  display: inline-block;
  margin-left: 5rpx;
}
.deleteBtn .deleteImg{
  width: 60rpx;
  height: 60rpx;
  text-align: center
}
.noComment{
  height: 360rpx;
  text-align: center;
  line-height: 360rpx;
  color: #999999;
  font-size: 28rpx
}
.like{
  /* height: 68rpx; */
  display: flex;
}
.like image{
  height: 48rpx;
  width: 48rpx;
  vertical-align: middle
}
.like .txt,
.pageview-box-right .txt{
  font-size: 24rpx;
  margin-left: 5rpx;
  /* line-height: 48rpx; */
  color: #333333;
  margin-top: 11rpx;
}
.like .txtActive,
.pageview-box-right .txtActive{
  font-size: 24rpx;
  margin-left: 5rpx;
  line-height: 48rpx;
  color:#ffaa33;
}
.content .textCot{
  width: 602rpx;
  display: block;
  margin-left: 88rpx;
  word-wrap:break-word;
  font-size: 28rpx;
  margin-top: 25rpx;
  margin-bottom: 40rpx;
}

.block{
  height: 99rpx;
}







.reply{
 height: 98rpx;
  width: 750rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  border-top: 1rpx solid #e8e8e8;
}
.replyTxt{
  background-color: #f5f5f5;
  width: 440rpx;
	height: 64rpx;
  border-radius: 31rpx;
  margin-left: 30rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  color: #999999;
  text-align: left;
  padding-left: 20rpx;
}
.reply .untily{
  position: relative;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
}
.reply .replyIcon{
  width:56rpx;
  height:56rpx;
}


.replyTxt2{
  width:500rpx;
  height:64rpx;
  border-radius:31rpx;
  margin-left:22rpx;
  line-height:64rpx;
}
