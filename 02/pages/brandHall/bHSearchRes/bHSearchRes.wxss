page {
  background-color: #fff;
}

.saerch{
  position: fixed;
  top:0;
  left: 0;
  z-index: 99;
  width: 100%;
  height: 98rpx;
  background-color: #fff;
}

.saerch-btn{
  position: absolute;
  top: 20rpx;
  z-index: 3;
  width: 600rpx;
  height: 58rpx;
  margin-left: 30rpx;
  background-color: #f5f5f5;
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #999999;
  line-height: 58rpx;
  display: flex;
  justify-content: flex-start;
  transition: all 0.1s linear;
}
.saerch-btn-active{
  width: 690rpx;
}

.saerch-btn input{
  width: 100%;
  height:58rpx;
  line-height: 58rpx;
}

.search-text{
  position: absolute;
  top: 30rpx;
  right: 40rpx;
  z-index: 2;
  font-size: 28rpx;
  color: #ff6419;
  transition: all 0.2s linear;
}
.search-text-active{
  position: absolute;
  right: 70rpx;
  top: 30rpx;
  z-index: 2;
  font-size: 28rpx;
  color: #ff6419;
  transition: all 0.2s linear;
}
.saerch-btn image{
  width: 30rpx;
  height: 30rpx;
  margin: 17rpx 14rpx 0 30rpx;
}

/* 搜索历史，热门搜索 */
.search-record{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 78rpx;
  z-index: 10;
  background-color: #fff;
  /* margin-top:  78rpx; */
}
.hot-search{
  border-bottom: 1rpx solid #f5f5f5;
}
.search-title{
  padding: 30rpx;
  font-size: 28rpx;
  color: #999999;
  display: flex;
  justify-content: space-between;
}
.search-list{
  padding: 0 30rpx 20rpx;
}
.search-item{
  display: inline-block;
  margin: 0 20rpx 20rpx 0;
  padding: 15rpx 25rpx;
  background-color: #f5f5f5;
  border-radius: 8rpx;
  font-size: 26rpx;
  color: #333333;
}
.delete{
  width: 44rpx;
  height: 44rpx;
}

.header_top {
  margin: auto;
}

.header_top img {
  display: block;
  width: 100%;
  height: auto;
}

.main_body {
  margin: auto;
}

.header {
  text-align: center;
  height: 90rpx;
  position: relative;
  background-color: #fff;
}

.header_title {
  line-height: 90rpx;
  font-size: 32rpx;
  color: #323232;
  width: 50%;
  float: left;
  position: relative;
}

.header_title .txt {
  color: #333;
}

.header_active .txt {
  color: #ff6419;
  font-weight: 600;
}

.header_active:after {
  content: "";
  position: absolute;
  bottom: 0rpx;
  height: 8rpx;
  left: 50%;
  margin-left: -16rpx;
  background-color: #ff6419;
  display: block;
  width: 32rpx;
  border-radius: 4rpx;
}

.header_xian {
  height: 32rpx;
  width: 2rpx;
  background: #e8e8e8;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -16rpx;
}

.spppg_banner {
  padding: 30rpx 4%;
  overflow: hidden;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.spppg_banner swiper {
  width: 100%;
  height: 200rpx;
}

.spppg_banner swiper image {
  border-radius: 12rpx;
}

.spppg_fenlei {
  padding: 30rpx 4%;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.spppg_fenlei .fenlei_item {
  width: 33.333%;
  float: left;
  text-align: center;
}

.spppg_fenlei .fenlei_item image {
  display: block;
  height: 96rpx;
  width: 96rpx;
  margin: auto;
}

.spppg_fenlei_text {
  display: block;
  font-size: 28rpx;
  color: #333;
  padding-top: 20rpx;
}

.spppg_dzss {
  padding: 30rpx 4% 0rpx 4%;
  background-color: #fff;
}

.sousuo_bottom {
  border-bottom: 2rpx solid #e8e8e8;
}

.spppg_search {
  width: 100%;
  position: relative;
  display: block;
  height: 60rpx;
  background-color: #f6f6f6;
  border-radius: 30rpx;
}

.spppg_search input {
  width: 85%;
  height: 60rpx;
  border: none;
  background: none;
  color: #333 !important;
  outline: none;
  font-size: 26rpx;
  position: absolute;
  left: 80rpx;
  top: 0rpx;
}

.spppg_search .search_btn {
  cursor: pointer;
  position: absolute;
  top: 4rpx;
  left: 9rpx;
  display: block;
  padding: 4rpx 20rpx;
}

.spppg_search image {
  height: 28rpx;
  width: 28rpx;
}

.spppg_dzss .am-dropdown {
  position: relative;
  display: inline-block;
  width: 20%;
}

.spppg_dzss .am-dropdown-content {
  min-width: 100% !important;
  padding: 0rpx !important;
  margin: 16rpx 0 0 0 !important;
  border: 2rpx solid #e8e8e8;
  text-align: center !important;
  border-radius: 6rpx !important;
}

.search_none_text {
  line-height: 48rpx;
  font-size: 28rpx;
  color: #ff6419;
  text-align: center;
  padding-top: 30rpx;
  border-bottom: 2rpx solid #e8e8e8;
  padding-bottom: 30rpx;
}

/*全部  */

.spppg_neiro {
  margin-top: 78rpx;
  padding: 30rpx 30rpx;
  background-color: #fff;
  position: relative;
  margin-bottom: 20rpx;
}

.spppg_quanbu {
  padding: 30rpx 4% 0rpx 4%;
}

.spppg_quanbu>.am-dropdown {
  display: inline-block;
}

.spppg_quanbu .am-dropdown-content {
  min-width: 100% !important;
  text-align: center;
  padding: 0rpx 0rpx 20rpx 0rpx !important;
  margin: 30rpx 0 0 !important;
  border: 0rpx;
}

.spppg_quanbu .am-dropdown {
  width: 100%;
}

.spppg_quanbu .am-dropdown-content li {
  width: 32%;
  float: left;
  margin-right: 2%;
  margin-bottom: 20rpx;
  display: block;
}

.spppg_quanbu .am-dropdown-content li:nth-child(3n) {
  margin-right: 0rpx;
}

.spppg_quanbu .am-dropdown-content:before {
  display: none;
}

.spppg_quanbu .am-dropdown-content:after {
  display: none;
}

.spppg_quanbu .am-dropdown-toggle {
  font-size: 32rpx;
  color: #333;
  cursor: pointer;
  display: inline-block;
}

.spppg_quanbu .am-icon-caret-down {
  color: #999;
}

.spppg_quanbu .am-dropdown-content li a {
  display: block;
  font-size: 28rpx;
  color: #333;
  background-color: #f5f5f5;
  padding: 0rpx;
  margin: 0rpx;
  line-height: 80rpx;
  border-radius: 6rpx;
}

.spppg_quanbu .am-dropdown-content li a:focus {
  background-color: #fff1eb;
  color: #ff6419;
}

.spppg_active a {
  background-color: #fff1eb !important;
  color: #ff6419 !important;
}

.spppg_sxian {
  height: 24rpx;
  background-color: #e8e8e8;
  width: 1px;
  margin-top: 8rpx;
  margin-right: 40rpx;
  display: inline-block;
}

.spppg_xiaol {
  position: absolute;
  top: 30rpx;
  left: 4%;
  margin-left: 160rpx;
}

.spppg_xiaol .am-dropdown-content {
  min-width: 80rpx;
  white-space: nowrap;
  left: 30rpx;
  font-size: 28rpx;
  padding: 20rpx;
  line-height: 54rpx;
}

.spppg_xiaol .am-dropdown-toggle {
  font-size: 32rpx;
  color: #333;
  cursor: pointer;
}

.spppg_xiaol .am-icon-caret-down {
  color: #999;
}

.am-dropdown {
  display: inline-block;
}

.spppg_xiaol .am-dropdown .arrow_down {
  position: relative;
  top: 8rpx;
  margin-left: 6rpx;
}

/* .spppg_xiaol.am-active .arrow_down{transform: rotate(180deg); */

.spppg_sxuan {
  position: absolute;
  right: 4%;
  top: 30rpx;
}

.spppg_shaixuan {
  font-size: 32rpx;
  color: #666;
}

.spppg_shaixuan a:focus {
  color: #ff6419;
}

/*猜你喜欢  */

.spppg_bianju {
  height: 30rpx;
  background-color: #fff;
  border-bottom: 20rpx solid #f6f6f6;
}

.cnxh_text {
  text-align: center;
  display: block;
  position: relative;
  height: 48rpx;
  width: 50%;
  margin: auto;
  padding-top: 30rpx;
}

.cnxh_text_h1 {
  display: inline-block;
  background-color: #fff;
  padding: 0rpx 16rpx;
  position: relative;
  z-index: 9;
  line-height: 48rpx;
  font-size: 32rpx;
  color: #999;
}

.cnxh_text::before {
  content: "";
  display: block;
  position: absolute;
  bottom: 20rpx;
  height: 2rpx;
  width: 100%;
