  z-index: 8;
}

.spppg_xinxi {
  overflow: hidden;
  margin-bottom: 30rpx;
  background-color: #ffffff;
	box-shadow: 0rpx 10rpx 25rpx 4rpx rgba(102, 102, 102, 0.1);
	border-radius: 10rpx;
}

.spppg_xinxi:last-child {
  border-bottom: none;
}

.xinxi_logo {
  width: 128rpx;
  height: 128rpx;
  margin-right: 3%;
  display: inline-block;
  vertical-align: middle;
  overflow: visible;
}

.xinxi_logo image {
  display: inline-block;
  width: 128rpx;
  height: 128rpx;
  vertical-align: middle;
  max-width: 100%;
  border-radius: 6rpx;
  border: 2rpx solid #e8e8e8;
}

.xinxi_right {
  width: 400rpx;
  height: 128rpx;
  display: inline-block;
  vertical-align: middle;
}

.xinxi_title {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}

.xinxi_text {
  height: 48rpx;
  line-height: 48rpx;
  color: #999;
  font-size: 26rpx;
  margin-top: 5rpx;
}

.xinxi_neiro {
  font-size: 26rpx;
  margin-top: 5rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.spppg_xinxi-img{
  width: 100%;
  height: 388rpx;
}

.xinxi_jieshao {
  position: relative;
  overflow: hidden;
  padding: 40rpx;
}

.xinxi_haop {
  position: absolute;
  right: 0rpx;
  top: 50%;
  margin-top: -24rpx;
}

.xinxi_haop {
  line-height: 48rpx;
  height: 48rpx;
  color: #ff6419;
  font-size: 28rpx;
}

.xinxi_pic {
  overflow: hidden;
  width: 100%;
  padding-top: 20rpx;
}

.xinxi_pic .pic_item {
  float: left;
  width: 32%;
  margin-right: 2%;
  margin-bottom: 20rpx;
}

.xinxi_pic .pic_item:nth-child(3n) {
  margin-right: 0rpx;
}

.xinxi_pic .pic_item image {
  display: block;
  max-width: 100%;
  height: auto;
  border-radius: 6rpx;
}

