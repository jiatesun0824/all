.dialog-content, .render-content, .select-space {
  width: 540rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: fixed;
  top: 400rpx;
  left: 105rpx;
}

.dialog-body {
  display: flex;
  flex-direction: column;
  padding: 60rpx 60rpx 40rpx;
}

.dialog-title {
  line-height: 30rpx;
  margin-bottom: 38rpx;
  font-size: 36rpx;
  font-weight: 600;
  text-align: center;
  color: #29cccc;
}

.dialog-msg {
  font-size: 28rpx;
  line-height: 48rpx;
}

.dialog-footer {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  border-top: 1rpx solid #efefef;
}

.new-plan, .replace-plan {
  flex-basis: 50%;
  text-align: center;
  box-sizing: border-box;
}

.new-plan {
  border-right: 1rpx solid #efefef;
}

.replace-plan {
  color: #29cccc;
}

.replace-plan2 {
  flex-basis: 100%;
  text-align: center;
  box-sizing: border-box;
  color: #29cccc;
}