page {
    display: block;
    min-height: 100%;
    background-color: #eeeeee;
}
.block{
	
	background: #fff;
	overflow: hidden;
	margin-bottom:20rpx;
}
.block-top{
	padding: 0 32rpx;
	position: relative;
}
.words{
	margin-top: 65rpx;
	width: 100%;
	color: #333333;
	font-size: 32rpx;
}
.nums{
	width: 100%;
	margin-top: 28rpx;
	margin-bottom:66rpx;
	font-size: 120rpx;
	color: #ff6419;
}
.nums text{
	font-size: 28rpx;
	color: #999999;
	
}
.bg{
	display: block;
	position: absolute;
	top:89rpx;
	right: 0rpx;
	width: 174rpx;
	height: 146rpx;
	z-index: 1;
}
.go{
	position: absolute;
	top: 195rpx;
	right: 0rpx;
	width:199rpx;
	height: 60rpx;
	background: #ff6419;
	color: #ffffff;
	font-size: 28rpx;
	
	line-height: 60rpx;
	border-radius: 30rpx 0 0 30rpx;
	z-index: 1;
}
.block{
	font-size: 28rpx;
}
.block-title{
	padding:32rpx 49rpx;
	color: #999999;
	border-bottom:1rpx solid #e8e8e8;
}
.block-iteam{
	width: 699rpx;
	margin-left: 51rpx;
	padding:37rpx 0;
	border-bottom:1rpx solid #e8e8e8;
	color: #333333;
}
.block-iteam:last-child{
	border: none;
}
.block-iteam-top{
	font-size: 28rpx;

}
.block-iteam-bottom{
	margin-top: 18rpx;
	font-size: 20rpx;
	color: #999;
}
.block-iteam .fr{
	margin-right: 49rpx;
	font-size: 32rpx;
}
.block-iteam .fr.red{
	color: #ff6419;
}
.goimg{
	width: 32rpx;
	height: 32rpx;
	vertical-align: -3rpx;
	margin-left: 37rpx;
	margin-right: 17rpx;
}
.noInformation{
  width:180rpx;
  height:180rpx;
  margin-left: 285rpx;
  margin-top:100rpx;
}
.txt{
  color:#999;
text-align:center;
font-size:28rpx;
width:750rpx;
