  /* 头部套餐卡部分 */
.my-accout .accout-combo {
  padding:30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.accout-combo .common{
  width: 690rpx;
  height: 280rpx;
  position: relative;
  padding: 0 41rpx;
  box-sizing: border-box;
}
.accout-combo .combo-card{
  margin-bottom: 30rpx;
}
.accout-combo .combo-card image {
  box-shadow: 5rpx 15rpx 15rpx rgba(255, 140, 25, 0.1);
}
.accout-combo .common .card-message,.accout-combo .common .specimen-message{
  position: absolute;
}
.accout-combo .common .card-message{
  top: 57rpx;
  width: 608rpx;
}
.accout-combo .common .specimen-message{
  top:45rpx;
  width: 608rpx;  
}
.accout-combo .common image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  box-shadow: 5rpx 15rpx 15rpx rgba(255, 102, 25, 0.1);
}
.combo-card .card-message .message-title{
  font-size: 60rpx;
  line-height: 103rpx;
  color: #fff;
}
.combo-card .card-message .message-title text{
  font-size: 28rpx;
}
.combo-card .card-message .message-state{
  line-height: 60rpx;
}
.message-state{
  width: 100%;  
}
.card-message .message-state .state-mes{
  font-size: 32rpx;
  color: #fff;
}
.specimen-message .message-state .state-mes{
  font-size: 120rpx;
  line-height: 170rpx;
  color:#fff;
}

.message-state .state-btn{
  width: 160rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 30rpx;
  font-size: 28rpx;
  color: #ff9933;
  line-height: 60rpx;
  text-align: center;
}
.specimen-message .message-state .state-btn{
  margin-top: 79rpx;
}
.combo-specimen .specimen-message .message-title{
  font-size: 32rpx;
  color: #fff;
}
