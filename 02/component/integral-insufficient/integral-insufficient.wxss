.integral-insufficient{
  background:rgba(17, 17, 17, 0.3);
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.integral-insufficient-box{
  width: 540rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 375rpx auto 0;
}
.title{
  font-size: 36rpx;
  color: #ff6419;
  padding: 48rpx 0 20rpx;
  text-align: center;
}
.content{
  font-size: 28rpx;
  color: #000000;
  text-align: center;
}
.bindPhone{
  height: 48rpx;
  padding: 40rpx 50rpx 0;
  display: flex;
  justify-content: space-between;
  color: #ff6419; 
}
.bindPhone-text{
  font-size: 28rpx;
  line-height: 48rpx;
}
.bindPhone-btn{
  width: 130rpx;
  height: 48rpx;
  font-size: 24rpx;
  line-height: 48rpx;
  border: 1rpx solid #ff6419;
  border-radius: 24rpx;
  text-align: center;
}
.btn{
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  margin-top: 48rpx;
  border-top: 1rpx solid #f5f5f5; 
}
.left{
  width: 50%;
  height: 100%;
  line-height: 88rpx;
  color: #999999;
  text-align: center;
}
.right{
  width: 50%;
  height: 100%;
  line-height: 88rpx;
  text-align: center;
  color: #ff6419;
  border-left: 1rpx solid #f5f5f5; 
}