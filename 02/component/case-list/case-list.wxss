/* rzd181121随选网UI调整 start*/
.page_icon_cost {
  width: 56rpx;
	height: 56rpx;
  position: absolute;
  top: 0;
  left: 12rpx;
  z-index:1;
}
.page_view_cost {
  position: absolute;
  top: 0;
  left: 0;
  height: 52rpx;
  line-height: 52rpx;
  padding: 0 20rpx;
  background: rgba(0,0,0,0.4);
	border-radius: 10rpx 0rpx 10rpx 0rpx;
  z-index:1;
  font-size: 28rpx;
  font-weight: bold;
  color: #ffaa33;
}
.page_view_cost>image {
  width: 27rpx;
	height: 28rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.page_view_cost>text {
  vertical-align: middle;
}
.page_view_cost .isBuy{
  margin-left: 7rpx;
  font-size: 24rpx;
	color: #ffaa33;
}
.planCost_bg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 33;
}
.planCost_box {
  width: 540rpx;
	height: 560rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
  position: absolute;
  top: 50%;
  margin-top: -280rpx;
  left: 50%;
  margin-left: -270rpx;
}
.planCost_box>image {
  width: 540rpx;
	height: 200rpx;
}
.planCost_title {
  font-size: 28rpx;
  line-height: 46rpx;
  color: #333;
  text-align: center;
}
.planCost_title:first-of-type {
  margin-top: 28rpx;
}
.planCost_price {
  font-size: 28rpx;
  color: #ff8c19;
  text-align: center;
  margin-bottom: 30rpx;
}
.planCost_but {
  width: 420rpx;
	height: 80rpx;
	background-color: #ffaa33;
	border-radius: 40rpx;
  font-size: 32rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
}
.colletAndsup {
  width: 240rpx;
	height: 48rpx;
  line-height: 48rpx;
  display: flex;
  justify-content: space-around;
  position: absolute;
  right: 0;
  top: 20rpx;
}
.colletAndsup_item:first-child {
  width: 112rpx;
}
.colletAndsup_item:last-child {
  width: 120rpx;
}
.colletAndsup_item>text,.colletAndsup_item>image {
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
  vertical-align: middle;
  font-size: 24rpx;
  color: #999;
}
.colletAndsup_item>image {
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  margin-right: 2rpx;
}
