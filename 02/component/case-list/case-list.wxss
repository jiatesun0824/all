.clearfix:after {
    content: '';
    height: 0;
    line-height: 0;
    clear: both;
    visibility: hidden;
    display: block;
}

.clearfix {
    zoom: 1;
}

.fl {
    float: left;
}

.fr {
    float: right;
}
