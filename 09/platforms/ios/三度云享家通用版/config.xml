    <preference name="PaginationMode" value="unpaginated" />
    <preference name="loadUrlTimeoutValue" value="60000" />
    <preference name="alipayid" value="2018070360521269" />
    <preference name="WECHATAPPID" value="wxd4934d0dab14d276" />
