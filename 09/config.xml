<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sandu.cloudhome.common" ios-CFBundleVersion="3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>三度云享家通用版</name>
    <description>
        三度云享家通用版
    </description>
    <author email="zhengyoucai@sanduspace.cn" href="http://www.sanduspace.cn">
        SanDu Space Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-navigation href="*" />
        <allow-intent href="*" />
        <access origin="*" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="orientation" value="portrait" />
    </platform>
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <chcp>
        <auto-download enabled="false" />
        <auto-install enabled="false" />
        <config-file url="https://mm.sanduspace.com/hotupdate/ios/chcp.json" />
    </chcp>
    <plugin name="cordova-plugin-wechat" spec="^2.3.0">
        <variable name="WECHATAPPID" value="wxd4934d0dab14d276" />
    </plugin>
    <plugin name="cordova-plugin-alipay-v2" spec="0.0.4">
        <variable name="APP_ID" value="2018070360521269" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <engine name="ios" spec="^4.5.4" />
</widget>
